{
  "name": "proton-i18n",
  "version": "1.4.0",
  "description": "CLI to manage translations for client apps",
  "main": "index.js",
  "bin": {
    "proton-i18n": "index.js"
  },
  "scripts": {
    "lint": "eslint {lib,index.js,config.js} --ext .js --quiet --cache",
    "pretty": "prettier --write  $(find lib/ index.js -type f -name '*.js')",
    "test": "echo \"Error: no test specified\" && exit 1",
    "preversion": "git update-index --no-assume-unchanged package-lock.json",
    "postversion": "git update-index --assume-unchanged package-lock.json && git push --tags && git push"
  },
  "keywords": [],
  "engines": {
    "node": ">= 10.15.0"
  },
  "author": "Proton Technologies AG",
  "license": "ISC",
  "dependencies": {
    "angular-gettext-cli": "github:huston007/angular-gettext-cli#a59bc3e142a5bee1b21d1876d1bb88008bd115b7",
    "chalk": "^2.4.2",
    "csv-parse": "^4.4.1",
    "dedent": "^0.7.0",
    "dotenv": "^8.0.0",
    "execa": "^1.0.0",
    "form-data": "^2.3.3",
    "got": "^9.6.0",
    "jszip": "^3.2.1",
    "lodash": "^4.17.11",
    "minimist": "^1.2.0",
    "moment": "^2.24.0",
    "ora": "^3.4.0",
    "ttag-cli": "^1.7.13"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^5.12.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-react-app": "^4.0.1",
    "eslint-plugin-flowtype": "^3.9.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-react-hooks": "^1.0.2",
    "husky": "^2.3.0",
    "lint-staged": "^8.1.7",
    "prettier": "^1.17.1"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
